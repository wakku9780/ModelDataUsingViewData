@{
     ViewData["Title"] = "Home Page";
    // var emp = ViewData["myEmployee"] as Employee;
    //var emp = ViewBag.myEmployee;
    //var emp = TempData["myEmployee"] as Employee;


    //var emps = ViewData["myEmployee"] as List<Employee>;
    //var emps = ViewBag.myEmployee;
    var emps = TempData["myEmployee"] as List<Employee>;

}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>


@{
    if (emps != null)
    {
        foreach (var emp in emps)
        {
                     <h2>ID:@emp.EmpId</h2>
                    <h2>NAME:@emp.EmpName</h2>
                    <h2>DESIGNATION:@emp.Designation</h2>
                    <h2>SALARY:@emp.Salary</h2>
                    <p>---------------</p>
                    
            
        }
                 
    }
}

